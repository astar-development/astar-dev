        Width="400" Height="450" Background="DarkSeaGreen" Foreground="White"
        Title="Graph Login Example">

    <DockPanel>
        <TextBlock Text="{Binding Status}" FontSize="18" />
        <Button Content="Sign In" Command="{Binding SignInCommand}" />
    </DockPanel>

</Window>