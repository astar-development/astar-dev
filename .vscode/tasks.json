{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "aspire-build",
            "type": "process",
            "command": "dotnet",
            "args": [
                "build",
                "${workspaceFolder}/src/aspire/AStar.Dev.Web.AppHost/AStar.Dev.Web.AppHost.csproj",
                "-c",
                "Debug",
                "-f",
                "net10.0"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "always",
                "panel": "shared"
            }
        },
        {
            "label": "onedrive-build",
            "type": "process",
            "command": "dotnet",
            "args": [
                "build",
                "${workspaceFolder}/src/services/AStar.Dev.OneDrive.Client/AStar.Dev.OneDrive.Client.csproj",
                "-c",
                "Debug",
                "-f",
                "net10.0"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "always",
                "panel": "shared"
            }
        }
    ]
}